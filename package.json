{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "yarn test:lint && yarn test:flow && yarn test:unit",
    "test:unit": "jest --maxWorkers=2",
    "test:lint": "eslint . --quiet",
    "test:watch": "jest --watch",
    "test:flow": "yarn flow",
    "test:flow:coverage": "flow-coverage-report --threshold=75 -i 'src/**/*.{js,jsx}'"
  },
  "dependencies": {
    "moment": "^2.22.2",
    "react": "16.4.1",
    "react-native": "0.56.0",
    "react-navigation": "^2.12.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/react": "^16.4.14",
    "@types/react-native": "^0.56.21",
    "@types/react-redux": "^6.0.9",
    "@types/react-test-renderer": "^16.0.2",
    "babel-jest": "23.4.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "^5",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.5.0",
    "eslint-config-bambi": "^1.4.0",
    "jest": "23.5.0",
    "prettier": "^1.14.2",
    "react-native-storybook": "^1.0.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.4.1",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "bambi"
  }
}
